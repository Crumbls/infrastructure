{
    "name": "crumbls/infrastructure",
    "description": "A Laravel package for infrastructure mapping and visualization",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Crumbls\\Infrastructure\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Crumbls",
            "email": "developer@crumbls.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "kalnoy/nestedset": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crumbls\\Infrastructure\\InfrastructureServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
